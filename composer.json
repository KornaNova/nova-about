{
    "name": "rhyslees/nova-about",
    "description": "This is a package to add about information for Laravel Nova",
    "keywords": [
        "RhysLees",
        "laravel",
        "nova-about"
    ],
    "homepage": "https://github.com/rhyslees/nova-about",
    "license": "MIT",
    "authors": [
        {
            "name": "Rhys Lees",
            "email": "43909932+RhysLees@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "repositories": {
        "nova":{
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "laravel/nova": "~4.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "RhysLees\\NovaAbout\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RhysLees\\NovaAbout\\NovaAboutServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
